<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cjj.easy.activity" >

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:name="com.cjj.easy.model.VolleyFrameApplication"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cjj.easy.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>

        <activity android:name="com.cjj.easy.activity.StringRequestActivity" >
        </activity>

        <activity android:name="com.cjj.easy.activity.JsonRequestActivity" >
        </activity>

        <activity android:name="com.cjj.easy.activity.ImageRequestActivity" >
        </activity>

        <activity android:name="com.cjj.easy.activity.ImageLoaderActivity" >
        </activity>

        <activity android:name="com.cjj.easy.activity.NetworkImageViewActivity" >
        </activity>

        <activity android:name="com.cjj.easy.activity.XmlRequestActivity" >
        </activity>

        <activity android:name="com.cjj.easy.activity.GsonRequestActivity" >
        </activity>
    </application>

</manifest>
